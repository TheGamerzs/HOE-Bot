{
	"env": {
		"browser": false,
		"es2021": true
	},
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"root": true,
	"plugins": ["@typescript-eslint"],
	"rules": {
		"indent": ["error", "tab"],
		"max-len": ["error", { "code": 120 }]
	}
}
