{
	"env": {
		"browser": false,
		"es2021": true
	},
	"extends": ["strongloop"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["@typescript-eslint"],
	"rules": {
		// Tab indentation
		"indent": ["error", "tab"]
	}
}
